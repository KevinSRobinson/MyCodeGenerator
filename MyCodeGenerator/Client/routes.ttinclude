
<#+ 

  void GeneratRoutes(string appPath, string className, string cClassName){			
		#>
		

		
var config = function($stateProvider) {

  $stateProvider
    .state("<#=cClassName#>s", {
      url: "/<#=cClassName#>s",
	  controller : "<#=cClassName#>Controller",
	  controllerAs : "vm",
      templateUrl: "/app/<#=cClassName#>s/Views/Index.html"
    });
  
};


	

angular.module("app").config(config);
	
		
		<#+

CreateFile(appPath + "Output/app/" + className + "s/routes.js");
	
	}	
#>

